{
  "title": "WordPress Sites",
  "tags": [
    "railway",
    "wordpress"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests por Sitio",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (project) (count_over_time({service=\"wordpress\"} |~ \"GET|POST\"[1m]))",
          "refId": "A",
          "legendFormat": "{{project}}"
        },
        {
          "expr": "sum(count_over_time({service=\"abemon.es\"} |~ \"GET|POST\"[1m]))",
          "refId": "B",
          "legendFormat": "abemon.es"
        }
      ],
      "datasource": {
        "type": "loki",
        "uid": "loki"
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Tiempo de Respuesta (ABM_DIAG)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "{project=\"abemon.es\", service=\"abemon.es\"} |~ \"REQUEST COMPLETE\" | regexp \"Total: (?P<time>[0-9.]+)ms\"",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "loki",
        "uid": "loki"
      }
    },
    {
      "id": 3,
      "type": "logs",
      "title": "abemon.es Logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "{project=\"abemon.es\", service=\"abemon.es\"}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "options": {
        "showTime": true,
        "showLabels": false,
        "wrapLogMessage": true
      }
    },
    {
      "id": 4,
      "type": "logs",
      "title": "BM Consulting Logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "{project=\"BM Consulting\", service=\"wordpress\"}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "options": {
        "showTime": true,
        "showLabels": false,
        "wrapLogMessage": true
      }
    },
    {
      "id": 5,
      "type": "logs",
      "title": "blue-mountain.es Logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "{project=\"blue-mountain.es\", service=\"wordpress\"}",
          "refId": "A"
        }
      ],
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "options": {
        "showTime": true,
